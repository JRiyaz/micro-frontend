{
  "name": "inventory",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:shell": "ng serve --project shell",
    "start:user": "ng serve --project user",
    "start:all": "concurrently \"ng serve --project shell\" \"ng serve --project user\" ",
    "run:all": "concurrently \"ng serve --project shell\" \"ng serve --project user\" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "postinstall": "sh submodule-husky-hook-path.sh"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/native-federation": "^19.0.7",
    "@angular/animations": "^19.1.2",
    "@angular/common": "^19.1.2",
    "@angular/compiler": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/forms": "^19.1.2",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "@ngrx/signals": "^19.0.0",
    "es-module-shims": "^2.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.2",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "husky": "^8.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.7.3"
  }
